{
    "name": "imagemin-webpack",
    "version": "1.1.2",
    "description": "Webpack loader and plugin to optimize (compress) images using imagemin",
    "repository": {
        "type": "git",
        "url": "https://github.com/itgalaxy/imagemin-webpack.git"
    },
    "homepage": "https://github.com/itgalaxy/imagemin-webpack",
    "keywords": [
        "webpack",
        "loader",
        "plugin",
        "imagemin",
        "images",
        "minify",
        "compress",
        "optimize"
    ],
    "author": "Itgalaxy <development@itgalaxy.company>",
    "bugs": {
        "url": "https://github.com/itgalaxy/imagemin-webpack/issues"
    },
    "license": "MIT",
    "main": "index.js",
    "files": [
        "utils",
        "imagemin-loader.js",
        "ImageminWebpackPlugin.js",
        "index.js"
    ],
    "engines": {
        "node": ">= 4.3 < 5.0.0 || >= 5.10"
    },
    "dependencies": {
        "async-throttle": "^1.1.0",
        "loader-utils": "^1.0.0",
        "imagemin": "^5.3.1",
        "nodeify": "^1.0.0",
        "minimatch": "^3.0.3",
        "webpack-sources": "^1.0.0"
    },
    "devDependencies": {
        "ajv-cli": "^2.1.0",
        "ava": "^0.19.1",
        "babel-preset-es2015": "^6.22.0",
        "babel-preset-stage-0": "^6.22.0",
        "babel-register": "^6.23.0",
        "eslint": "^3.17.0",
        "eslint-plugin-ava": "^4.2.1",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-itgalaxy": "^47.0.0",
        "eslint-plugin-jest": "^20.0.3",
        "eslint-plugin-jsx-a11y": "^5.0.1",
        "eslint-plugin-lodash": "^2.4.3",
        "eslint-plugin-node": "^5.0.0",
        "eslint-plugin-prettier": "^2.1.2",
        "eslint-plugin-promise": "^3.5.0",
        "eslint-plugin-react": "^7.1.0",
        "eslint-plugin-unicorn": "^2.1.2",
        "file-loader": "^0.11.1",
        "husky": "^0.13.0",
        "imagemin-gifsicle": "5.1.0",
        "imagemin-mozjpeg": "^6.0.0",
        "imagemin-pngquant": "^5.0.0",
        "imagemin-svgo": "^5.2.0",
        "lint-staged": "^4.0.0",
        "lodash.defaultsdeep": "^4.6.0",
        "npm-run-all": "^4.0.0",
        "npmpub": "^3.1.0",
        "nyc": "^11.0.0",
        "package-schema": "^1.0.0",
        "pify": "^3.0.0",
        "prettier": "^1.4.4",
        "remark-cli": "^3.0.0",
        "remark-preset-lint-itgalaxy": "^6.0.0",
        "tempy": "^0.1.0",
        "webpack": "^3.0.0"
    },
    "peerDependencies": {
        "webpack": "^2.0.0 || ^3.0.0"
    },
    "scripts": {
        "precommit": "lint-staged",
        "lint:eslint": "eslint . --cache --ignore-path .gitignore",
        "lint:remark": "remark . -f -i .gitignore -q",
        "lint": "npm-run-all -l -p 'lint:**'",
        "formatter:prettier": "eslint . --fix --ignore-path .gitignore",
        "formatter": "npm-run-all -l -p formatter:**",
        "ava": "nyc ava --verbose",
        "pretest": "npm run lint",
        "test": "npm run ava",
        "release": "npmpub"
    },
    "ava": {
        "require": "babel-register",
        "concurrency": 5,
        "babel": "inherit"
    },
    "babel": {
        "presets": [
            "es2015",
            "stage-0"
        ]
    },
    "eslintConfig": {
        "parserOptions": {
            "sourceType": "script"
        },
        "rules": {
            "node/no-unsupported-features": "off"
        },
        "extends": [
            "plugin:itgalaxy/ava",
            "plugin:itgalaxy/node",
            "plugin:itgalaxy/esnext",
            "plugin:itgalaxy/prettier"
        ],
        "root": true
    },
    "remarkConfig": {
        "plugins": [
            "remark-preset-lint-itgalaxy"
        ]
    }
}
